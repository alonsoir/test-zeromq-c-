{
  "etcd_client": {
    "server": {
      "host": "127.0.0.1",
      "port": 2379,
      "timeout_seconds": 5
    },
    "component": {
      "name": "example-component",
      "config_path": "/path/to/component/config.json"
    },
    "encryption": {
      "enabled": true,
      "algorithm": "chacha20-poly1305",
      "key_source": "etcd-server"
    },
    "compression": {
      "enabled": true,
      "algorithm": "lz4",
      "min_size_bytes": 256
    },
    "heartbeat": {
      "enabled": true,
      "interval_seconds": 30
    },
    "retry": {
      "max_attempts": 3,
      "backoff_seconds": 2
    }
  }
}
