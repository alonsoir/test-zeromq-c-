// config/command_whitelist.json
{
  "allowed_commands": [
    "GET",
    "SET",
    "DELETE",
    "WATCH",
    "PUT",
    "LIST",
    "STATUS"
  ],
  "allowed_patterns": [
    "^[a-zA-Z0-9_./-]+$",
    "^config/",
    "^security/",
    "^ml-detector/"
  ],
  "restricted_keys": [
    "root",
    "admin",
    "password",
    "secret"
  ]
}