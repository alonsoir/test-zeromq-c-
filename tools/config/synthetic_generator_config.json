{
  "_header": "Synthetic Event Generator - Via Appia Quality",
  "_version": "1.0.0",
  "_description": "100% compliance with ml-detector: etcd + crypto + RAGLogger",

  "component": {
    "name": "synthetic_event_generator",
    "version": "1.0.0",
    "node_id": "generator-01"
  },

  "etcd": {
    "enabled": true,
    "endpoints": ["localhost:2379"],
    "connection_timeout_ms": 5000,
    "retry_attempts": 3,
    "crypto_token_path": "/crypto/ml-detector/tokens",
    "service_registration_path": "/services/synthetic-generator"
  },

  "generation": {
    "default_count": 100,
    "default_malicious_ratio": 0.20,
    "output_base": "/vagrant/logs/rag/synthetic"
  },

  "rag_logger": {
    "base_dir": "/vagrant/logs/rag/synthetic",
    "deployment_id": "synthetic-generator",
    "node_id": "generator-01",
    "min_score_to_log": 0.0,
    "save_protobuf_artifacts": true,
    "save_json_artifacts": false
  }
}